--Future by supergovnuk

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Future MM2 Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Future"})

local Tab = Window:MakeTab({
	Name = "MM2",
	Icon = "rbxassetid://15475079065",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Section"
})

Tab:AddButton({
	Name = "Emote",
	Callback = function()
      		game.ReplicatedStorage.Remotes.Misc.PlayEmote:Fire("Zombie")
  	end    
})
