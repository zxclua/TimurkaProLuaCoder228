--Future by supergovnuk

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Future MM2 Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Future"})

local Tab = Window:MakeTab({
	Name = "MM2",
	Icon = "rbxassetid://15475079065",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Section"
})

Tab:AddButton({
	Name = "Emote",
	Callback = function()
      		game.ReplicatedStorage.Remotes.Misc.PlayEmote:Fire("zombie")
  	end    
})

Tab:AddButton({
	Name = "Reset all",
	Callback = function()
z = game.Players.LocalPlayer
f = z.Character.SprayPaint
for _, v in game.Players:GetChildren() do
if v ~= z then
if v.Character then
if v.Character:FindFirstChild("HumanoidRootPart") then
f.Remote:FireServer(0, Enum.NormalId.Front, 2048, v.Character:FindFirstChild("HumanoidRootPart"), CFrame.new(0, -1000, 0)) 
end
end
end
end
  	end    
})
