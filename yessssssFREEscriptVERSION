game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/join dead", "alive")
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnChannelLeft.OnClientEvent:Connect(function(chnl)
    if chnl == "dead" then
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/join dead", "alive")
    end
end)
  	end    
