    [1] = 16047815243,
    [2] = Enum.NormalId.Front,
    [3] = 6,
    [4] = v.Character.HumanoidRootPart,
    [5] = v.Character.HumanoidRootPart.CFrame * CFrame.new(0,5,0)
}

