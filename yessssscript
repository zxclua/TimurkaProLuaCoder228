Tab:AddToggle({
	Name = "Player pet(ghost perk needed)",
	Default = false,
	Callback = function(Value)
getgenv().PlayerPet = Value

local args = {
        [1] = Value
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Gameplay"):WaitForChild("Stealth"):FireServer(unpack(args))

while PlayerPet == true do
    local args = {
        [1] = ".gg/e7uXsSCV9S"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Inventory"):WaitForChild("RenamePet"):FireServer(unpack(args))

game:GetService("RunService").RenderStepped:Wait()
end
	end    
})

